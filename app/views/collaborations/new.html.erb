<h1>Add a Collaborator To Your Private Wiki</h1>
<div class="col-md-8">
	<%= form_for @collaboration do |f| %>
		<div class="field">
			<%= f.label :email %><br />
			<%= f.email_field :email, autofocus: true %>
		</div>
		<div class="form-group">
		 	<%= f.submit "Add Collaborator", class: 'btn btn-success' %>
		</div>
	<% end %> 
</div>